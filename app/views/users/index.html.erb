<p style="color: green"><%= notice %></p>

<h1>Users</h1>
<table class="table">
  <thead>
    <tr>
      <th scope="col">Name</th>
      <th scope="col">Email</th>
      <th scope="col">Address</th>
      <th scope="col">Phone</th>
      <th scope="col">Action</th>
    </tr>
  </thead>
   <tbody>
<div id="users">
  <% @users.each do |user| %>
    <tr><%= render user %>
    
      <td><%= link_to "Show this user", user %></td>
    </tr>
  <% end %>
</div>
</tbody>
</table>
<%= link_to "New user", new_user_path %>

<%== pagy_nav(@pagy)%>
